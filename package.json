{
  "name": "BoilerplateMobileApp",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codecov": "codecov",
    "typecheck": "tsc",
    "install:pods": "pod install --project-directory=ios",
    "bump": "bundle exec fastlane bump",
    "bump:patch": "yarn bump version:patch",
    "bump:minor": "yarn bump version:minor",
    "bump:major": "yarn bump version:major"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.64.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/runtime": "^7.14.0",
    "@boilerz/eslint-config": "2.0.30",
    "@boilerz/jest-config": "^1.0.9",
    "@boilerz/prettier-config": "^1.0.4",
    "@boilerz/tsconfig": "^1.4.10",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.9",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.0.2",
    "codecov": "^3.8.2",
    "eslint": "^7.27.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.3.2"
  }
}
