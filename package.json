{
  "name": "BoilerplateMobileApp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codecov": "codecov",
    "typecheck": "tsc",
    "install:pods": "pod install --project-directory=ios",
    "bump": "bundle exec fastlane bump",
    "bump:patch": "yarn bump version:patch",
    "bump:minor": "yarn bump version:minor",
    "bump:major": "yarn bump version:major"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/runtime": "^7.13.7",
    "@boilerz/eslint-config": "2.0.17",
    "@boilerz/jest-config": "^1.0.5",
    "@boilerz/prettier-config": "^1.0.3",
    "@boilerz/tsconfig": "^1.4.7",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.63.50",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.2.2"
  }
}
